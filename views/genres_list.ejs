<%- include("header"); -%>

<h1><%= title %></h1>

<% if (genres.length) { %>
    <ul>
        <% for (const genre of genres) { %>
            <li><a href="<%= genre.url %>"><%= genre.name %></a></li>
        <% } %>
    </ul>
<% } else { %>
    <p>There are no genres in this library.</p>
<% } %>


<%- include("footer"); -%>